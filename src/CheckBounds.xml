	CheckBounds := lower;
	CheckBounds_ErrCnt:=CheckBounds_ErrCnt+1;
ELSIF  index &gt; upper THEN
	CheckBounds := upper;
	CheckBounds_ErrCnt:=CheckBounds_ErrCnt+1;
ELSE  
	CheckBounds := index;
END_IF
</xhtml>
